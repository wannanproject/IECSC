<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1"/>
    </startup>
    <appSettings>
      <!--OPC配置  需把服务端属性 槽修改为1【以下位机为准】-->
      <add key="OpcServerName" value="Kepware.KEPServerEX.V6"/>
      <add key="OpcGroupName" value="RFID_ORACLE_WPF"/>
      <add key="OpcServerIP" value="127.0.0.1"/>
      <!--输送线区域-->
      <add key="LocArea" value="B"/>
      <add key="LocIp" value="127.0.0.1"/>
      <!--SQL SERVER数据库配置-->
      <add key="DbIp" value="127.0.0.1"/>
      <add key="DbConnect" value="Data Source=(DESCRIPTION=(ADDRESS=(PROTOCOL=TCP)(HOST=192.168.31.196)(PORT=1521))(CONNECT_DATA=(SERVICE_NAME=orcl)));Persist Security Info=True;User Id=wnasrs;Password=wnasrs;" />

    </appSettings>
</configuration>

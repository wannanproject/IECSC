<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1"/>
    </startup>
    <appSettings>
      <!--OPC配置  需把服务端属性 槽修改为1【以下位机为准】-->
      <add key="OpcServerName" value="Kepware.KEPServerEX.V6"/>
      <add key="OpcGroupName" value="TRANS"/>

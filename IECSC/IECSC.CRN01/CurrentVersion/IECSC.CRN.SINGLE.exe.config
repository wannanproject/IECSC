<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
  </startup>
  <appSettings>
    <!--OPC配置  需把服务端属性 槽修改为1【以下位机为准】-->
    <add key="OpcSeverName" value="Kepware.KEPServerEX.V6"/>
    <add key="OpcGroupName" value="CRN"/>
    <add key="OpcServerIP" value="127.0.0.1"/>
    <!--堆垛机配置-->
    <add key="CrnName" value="CRN01"/>
    <!--数据库配置-->
    <add key="OrlConnect" value="Data Source=(DESCRIPTION=(ADDRESS=(PROTOCOL=TCP)(HOST=127.0.0.1)(PORT=1521))(CONNECT_DATA=(SERVICE_NAME=ORCL)));Persist Security Info=True;User ID=wnasrs;Password=wnasrs;"/>
  </appSettings>
</configuration>

<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.1" />
    </startup>
    <appSettings>
      <!--OPC配置  需把服务端属性 槽修改为1【以下位机为准】-->
      <add key="OpcServerName" value="Kepware.KEPServerEX.V6"/>
      <add key="OpcGroupName" value="SRM01"/>
      <add key="OpcServerIP"  value="127.0.0.1"/>
      <!--堆垛机配置-->
      <add key="SrmNo" value="CRN01"/>
      <add key="SrmIp" value="127.0.0.1"/>
      <!--数据库配置-->
      <add key="DbIp" value="127.0.0.1"/>
      <add key="DbConnect" value="Data Source=(DESCRIPTION=(ADDRESS=(PROTOCOL=TCP)(HOST=127.0.0.1)(PORT=1521))(CONNECT_DATA=(SERVICE_NAME=ORCL)));Persist Security Info=True;User ID=hbasrs;Password=hbasrs;"/>
    </appSettings>
</configuration>